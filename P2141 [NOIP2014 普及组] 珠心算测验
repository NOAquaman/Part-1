#include <iostream>
#include <cstdio>

using namespace std;

int main()
{
	int n = 0,ans = 0;
	int a[100] = {};
	bool b[100] = {};
	cin >> n;
	for(int i = 0;i<n;i++)
	{
		cin >> a[i];
	}
	for(int i = 0;i<n;i++)
	{
		for(int j = 0;j<n;j++)
		{
			for(int k = 0;k<n;k++)
			{
				if(i!=j&&i!=k&&j!=k)	
				{
					if(a[i]+a[j]==a[k])		b[k] = true;
				}
			}
		}
	}
		for(int i = 0;i<n;i++)
	{
		if(b[i] == true)	ans++;
	}
	cout << ans;
} 
