#include <iostream>
#include <cstdio>
#include <cmath>

using namespace std;

int main()
{
	int m=0,n=0,p=0; 
	int a[12]={};
	for(int i=0;i<=11;i++)
	{
		cin >> a[i];
	}
	for(int i=0;i<=11;i++)
	{
		m+=300;
		if((m-a[i])>=100)
		{
			n += m-a[i]-(m-a[i])%100;
			m = (m-a[i])%100;
		}
		else if(m-a[i]<0)
		{
			p=-i-1;
			break;
		}	
		else
		{
			m = m-a[i];
		}
	}
	if(p==0)	cout << n/5*6+m;
	else	cout << p;
	return 0;
} 
