#include <iostream>
#include <cstdio>

using namespace std;

bool day[1000][1000];

int main()
{
	day[1000][1000] = {};
	int m = 0,n = 0,k = 0,ans = 0,x = 0;
	cin >> n >> m >> k;
	for(int i = 0;i<n;++i)
	{
		for(int j=1;j<=m;++j)
        {
            cin >> x;
            day[x][j] = true;
        }
	}

	for(int i = 1;i<=k;++i)
	{
		for(int j = 1;j<=m;++j)
		{
			if(day[i][j]==true)		++ans;
		}
		cout << ans << " ";
		ans = 0;
	}
	return 0;
}
