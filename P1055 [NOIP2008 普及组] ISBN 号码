#include <iostream>
#include <cstdio>

using namespace std;

int main()
{
	char str[13] = {};
	int n = 0,sum = 0;
	cin >> str;
	for(int i = 0;i<12;++i)
	{
		if(str[i] != '-')
		{
			++n;
			sum += n * (str[i] - '0');
		}
	}
	sum %= 11;
	if(sum == 10)
	{
		if(str[12] == 'X')	cout << "Right";
		else
		{
			for(int i = 0;i<12;++i)	cout << str[i];
			cout << 'X';
		}
	}
	else
	{
		if(sum == (str[12] - '0'))
		{
			cout << "Right";
		}
		else
		{
			for(int i = 0;i<12;++i)	cout << str[i];
			cout << sum;
		}
	}
	return 0;
}
