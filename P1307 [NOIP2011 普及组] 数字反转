#include <iostream>
#include <cstdio>
#include <cstring>

using namespace std;

int main()
{
	char n[11] = {};
	int b = 0;
	scanf("%\s",n);
 	if(strlen(n)==1&&n[0]=='0') printf("0");
    else 
    {
      	if(n[0]=='-')		
	  	{
	  		n[0]='\n';
			printf("-");
		}
      	for(b=strlen(n)-1;b>=0;b--)	if(n[b]!='0') break;
      	for(;b>=0;b--)	printf("%c",n[b]);
    }
	return 0;
}
